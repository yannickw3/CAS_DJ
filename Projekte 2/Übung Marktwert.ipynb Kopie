    "# alles zusammenführen:\n",
    "\n",
    "allejahre = []\n",
    "jahre = list(range(2010,2019))\n",
    "\n",
    "for jahr in jahre:\n",
    "    url = \"https://www.transfermarkt.ch/1-bundesliga/startseite/wettbewerb/L1/plus/?saison_id=\"\n",
    "    url = (url+str(jahr))\n",
    "    headers = {'User-Agent': 'Mozilla/5.0 (Macintosh; Intel Mac OS X 10_10_1) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/39.0.2171.95 Safari/537.36'}\n",
    "    r = requests.get(url, headers=headers)\n",
    "    soup = BeautifulSoup(r.text, 'html.parser')\n",
    "           \n",
    "    for team, wert in zip(vereine, werte[2:][::2]):\n",
    "        team = team.text\n",
    "        wert = float(wert.text.replace(' Mio. €', '').replace(',', '.'))\n",
    "        dictionary = {'Team': team,\n",
    "                 'Wert': wert,\n",
    "                 'Saison':str(jahr)}\n",
    "        allejahre.append(dictionary)"
   ]
