       "<script type=\"text/javascript\">\n",
       "\n",
       "//Helper function\n",
       "function isIE() {\n",
       "  var myNav = navigator.userAgent.toLowerCase();\n",
       "  return (myNav.indexOf('msie') != -1) ? parseInt(myNav.split('msie')[1]) : false;\n",
       "} \n",
       "\n",
       "function addLoadingBox()\n",
       "{\n",
       "\t  \n",
       "\t$('#chart-container').prepend('<div class=\"loading\">'+loadingText+'</div>');\n",
       "}\n",
       "\n",
       "function drawChart()\n",
       "{\n",
       "    if(typeof height === 'undefined' || height == null)\n",
       "    {\n",
       "        height = 500;\n",
       "    }\n",
       "    \n",
       "    $('#chart-container').append('<div id=\"chart-canvas\" style=\"width: 100%; min-height:'+height+'px; \"></div>');\n",
       "    \n",
       "    // html5_chart: load the data and draw the chart\n",
       "    loadGraph(true);\n",
       "}\n",
       "\n",
       "\n",
       "function loadChart()\n",
       "{\n",
       "    $('#chart-load').html('<div id=\"chart-container\" ></div>');\n",
       "     \n",
       "    \n",
       "    if(typeof cssName === 'undefined' || cssName == null)\n",
       "    {\n",
       "        cssName = 'chart.css';    \n",
       "    }\n",
       "\t\n",
       "     $('<link rel=\"stylesheet\" type=\"text/css\" href=\"'+jsRoot+'css/chart/'+cssName+'?version=2\">').appendTo('head');\n",
       "\n",
       "     addLoadingBox();\n",
       "         \n",
       "     drawChart();\n",
       "    \n",
       "}\n",
       "\n",
       "\n",
       "$(function() {\n",
       "\tloadChart();\n",
       "});\n",
       "\n",
       "\n",
       "</script>\n",
